  "devDependencies": {
    "@textlint/types": "^13.3.0",
    "@types/ajv": "^1.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.13.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "textlint-scripts": "^13.3.0",
    "textlint-tester": "^13.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@textlint/regexp-string-matcher": "^2.0.2",
    "ajv": "^8.12.0",
    "eslint": "^8.34.0",
    "js-yaml": "^4.1.0",
    "textlint-rule-helper": "^2.3.0",
    "typescript-json-schema": "^0.55.0"
  }
}
