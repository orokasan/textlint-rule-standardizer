{
  "name": "@textlint-rule/textlint-rule-standardizer",
  "version": "0.1.1",
  "description": "fix spelling inconsistencies",
  "keywords": [
    "textlintrule"
  ],
  "homepage": "https://github.com/orokasan/textlint-rule-standardizer",
  "bugs": {
    "url": "https://github.com/orokasan/textlint-rule-standardizer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orokasan/textlint-rule-standardizer.git"
  },
  "license": "MIT",
  "author": "orokasan",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "src/"
  ],
  "scripts": {
    "test": "textlint-scripts test",
    "build": "textlint-scripts build",
    "prepublish": "npm run --if-present build"
